  function getVariantIDFromParts(sizeValue, colorValue) {
    const selectedParts = [sizeValue.trim().toLowerCase(), colorValue.trim().toLowerCase()];
  
    return var_json.find(variant => {
      const variantParts = variant.title.split(" / ").map(v => v.trim().toLowerCase());
      return selectedParts.every(part => variantParts.includes(part));
    });
  }

  addToCartBtn.addEventListener("click", async function(event) {
