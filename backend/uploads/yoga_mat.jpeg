        setTimeout(() => {
          errorMsg.style.display = "none";
        }, 3000);
        return;
      }
    
    // 1. Check if any product is sold out
      let soldOutVariant = null;
    
      for (const id of productIDs) {
        try {
          const res = await fetch(`/products/${id}.js`);
          const data = await res.json();
      
          if (!data.available) {
            soldOutVariant = data.title;
            break;
          }
        } catch (err) {
          console.error(`Error checking availability for variant ${id}:`, err);
        }
      }
    
      // 2. If sold out, show error and stop
